		-->     
		{% if third_party.is_set %}<div id="thirdparty">Booking as 3th party: {{ third_party.name }}</div>{% endif %}
		<div id="wrapper">
							<div id="flags" align="right">
									<a href="http://www.abberley.ie/">
											<img src="{{ "images/website/flag_en.gif" | skin_asset }}" alt="English" border="0" /></a>
									
