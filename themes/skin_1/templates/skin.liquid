	<head>
		{% assign common_version = 'v_001' %}
		<!-- assign test_asset = "http://vz-assets.s3.amazonaws.com/roomsandevents/abberley/skin_1/"	-->
		<meta http-equiv="Content-Type" content="text/html; charset=iso-8859-1" />
		<meta content="Oriana Pagano, Jesus Burgos Macia - Visrez" name="author" />
		<title>Abberley Court Hotel</title>
		<meta name="keywords" content="Abberley Court Hotel" />
		<meta name="Description" content="The Abberley Court Hotel located in Dublin, Tallaght offers unbeaten comfort in stylish surroundings." />

		<!-- STYLESHEETS -->
			{{ "website" | style_sheet }}
